<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>
<body>
<div class="container"></div>

<script>
    const postElement = document.getElementById('post-data');
    const postId = postElement?.dataset?.postId;
    document.addEventListener('DOMContentLoaded', async () => {
        try {
            const post = await fetchPostByIdAdmin(postId);
            if (!post) {
                renderSinglePostByIdAdmin(post);
            } else {
                console.error('No posts available to render.');
            }
        } catch (error) {
            console.error('Error fetching or rendering posts:', error);
        }
    });

</script>
<script src="../javascripts/admin.js"></script>
</body>


</html>
